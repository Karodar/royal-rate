{
    "manifest_version": 2,
    "name": "Web-F",
    "version": "0.0.1",
    "browser_action": {
        "default_icon": "app.png"
    },
    "background": {
        "scripts": [
            "app.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "http://*.marathonbet.com/*"
            ],
            "js": [
                "app.js"
            ]
        }
    ]
}